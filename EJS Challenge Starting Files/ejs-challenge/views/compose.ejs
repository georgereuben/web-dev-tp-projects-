<%- include('partials/header') %>

<h1>Compose</h1>

<form action="/compose" method="POST">
  <label>Title</label>
  <input type="text" class="form-control" name="postTitle" autocomplete="off">

  <label>Post</label>
  <textarea class="form-control" name="postContent" rows="3"></textarea>
  <br>
  <button type="submit" class="btn btn-primary" name="button">Publish</button> 
</form>

